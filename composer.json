{
    "name": "jeffreyvanrossum/flareforwp",
    "description": "This is my plugin flareforwp",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeffrey van Rossum",
            "email": "jeffrey@vanrossum.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "automattic/jetpack-autoloader": "^2.10",
        "jeffreyvanrossum/wp-settings": "^1.0",
        "spatie/ignition": "^1.12",
        "jeffreyvanrossum/wp-simple-encryption": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Jeffreyvr\\FlareForWp\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.0"
        },
        "allow-plugins": {
            "automattic/jetpack-autoloader": true
        }
    },
    "require-dev": {
        "laravel/pint": "^1.2"
    },
    "scripts": {
        "format": [
            "./vendor/bin/pint"
        ]
    }
}
